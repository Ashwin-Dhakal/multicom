then
	echo "need at least three parameters : target id, path of fasta sequence, directory of output"
	exit 1
fi

workdir=$1 #
targetid=$2 #
outputdir=$3 #




source SOFTWARE_PATH/tools/python_virtualenv/bin/activate
#export PATH=SOFTWARE_PATH/tools/R-3.2.0/bin/:$PATH
perl SOFTWARE_PATH/src/visualize_multicom_construct/P1_organize_prediction.pl  $workdir $targetid  $outputdir


